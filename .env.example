# ===========================================
# ADVAPI v2 - Exemplo de Configuração
# Copie para .env e ajuste os valores
# ===========================================

# PostgreSQL
POSTGRES_USER=advapi
POSTGRES_PASSWORD=sua-senha-forte-aqui
POSTGRES_DB=advapi

# Redis
REDIS_PASSWORD=sua-senha-redis-aqui

# JWT
JWT_SECRET=sua-chave-jwt-aqui
JWT_EXPIRES_IN=7d

# API Keys
API_KEY=sua-api-key-aqui
ADVWELL_API_KEY=sua-advwell-api-key-aqui

# CORS (domínios permitidos separados por vírgula)
CORS_ORIGIN=https://seu-dominio.com

# Ambiente
NODE_ENV=production
